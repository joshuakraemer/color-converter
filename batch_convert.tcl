set colors {
0xff 0xff 0xff
0xef 0xef 0xef
0xd6 0xd6 0xd6
0x4d 0x4d 0x4c
0x8e 0x90 0x8c
0xc8 0x28 0x29
0xf5 0x87 0x1f
0xea 0xb7 0x00
0x71 0x8c 0x00
0x3e 0x99 0x9f
0x42 0x71 0xae
0x89 0x59 0xa8
}

set L_sum 0
set C_sum 0

foreach {a b c} $colors {
	set color [exec -ignorestderr ~/Farbe/converter/converter sRGB Jab $a $b $c]
	set L [lindex $color 0]
	set C [lindex $color 1]
	set H [lindex $color 2]
	puts "$L\t$C\t$H"
	
	set L_sum [expr {$L_sum + $L}]
	set C_sum [expr {$C_sum + $C}]
	incr n
}

puts "mean L: [expr {$L_sum/$n}]"
puts "mean C: [expr {$C_sum/$n}]"